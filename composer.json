{
    "name": "belar/betaup",
    "description": "A Laravel 4 package for easy and comfortable pre-launch page setup",
    "authors": [
        {
            "name": "Belar",
            "email": "belar@belardesign.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models",
            "src/views",
            "src/facades"
        ],
        "psr-0": {
            "Belar\\Betaup": "src/"
        }
    },
    "minimum-stability": "stable"
}
